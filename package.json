{
  "name": "router-tutorial",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest --reporter=verbose",
    "format": "prettier --write --ignore-unknown .",
    "dep-a": "npm ci && npm audit fix && npm audit",
    "dep-c": "rm -rf node_modules",
    "dep-i": "npm install",
    "dep-r": "npm run dep-c && npm run dep-i && npm run dep-a"
  },
  "lint-staged": {
    "*.{js,css,json,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.8.2",
    "@emotion/styled": "^11.8.1",
    "@mui/icons-material": "^5.5.1",
    "@mui/material": "^5.5.3",
    "axios": "^0.26.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.3.0"
  },
  "devDependencies": {
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "@vitejs/plugin-react": "^1.0.7",
    "prettier": "^2.6.1",
    "typescript": "^4.5.4",
    "vite": "^2.9.0",
    "vitest": "^0.8.0"
  }
}
